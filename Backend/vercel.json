{
  "builds": [
    { "src": "/backend/**/*.js", "use": "@vercel/node" },
    { "src": "/frontend/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/backend/(.*)",
      "dest": "backend/$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "public/$1"
    },
    {
      "src": "/src/(.*)",
      "dest":"src/$1"
    }
  ]
}
